      // Ticket Modal
      #ticketModal.w3-modal
        .w3-modal-content.w3-animate-top.w3-card-4
          header.w3-container.w3-teal.w3-center.w3-padding-32
            span.w3-button.w3-teal.w3-xlarge.w3-display-topright(onclick="document.getElementById('ticketModal').style.display='none'") &times;
            h2.w3-wide
              i.fa.fa-suitcase.w3-margin-right
              | Tickets
          .w3-container
            p
              label
                i.fa.fa-shopping-cart
                |  Tickets, $15 per person
            input.w3-input.w3-border(type='text' placeholder='How many?')
            p
              label
                i.fa.fa-user
                |  Send To
            input.w3-input.w3-border(type='text' placeholder='Enter email')
            button.w3-button.w3-block.w3-teal.w3-padding-16.w3-section.w3-right
              | PAY 
              i.fa.fa-check
            button.w3-button.w3-red.w3-section(onclick="document.getElementById('ticketModal').style.display='none'")
              | Close 
              i.fa.fa-remove
            p.w3-right
              | Need 
              a.w3-text-blue(href='#') help?
      // The Contact Section
      #contact.w3-container.w3-content.w3-padding-64(style='max-width:800px')
        h2.w3-wide.w3-center CONTACT
        p.w3-opacity.w3-center
          i Fan? Drop a note!
        .w3-row.w3-padding-32
          .w3-col.m6.w3-large.w3-margin-bottom
            i.fa.fa-map-marker(style='width:30px')
            |  Chicago, US
            br
            i.fa.fa-phone(style='width:30px')
            |  Phone: +00 151515
            br
